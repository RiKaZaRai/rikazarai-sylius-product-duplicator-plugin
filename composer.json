{
    "name": "rikazarai/sylius-product-duplication-plugin",
    "type": "sylius-plugin",
    "description": "Sylius plugin to duplicate products in admin panel with bulk actions support",
    "keywords": [
        "sylius",
        "sylius-plugin", 
        "product",
        "duplication",
        "e-commerce",
        "admin",
        "bulk-actions"
    ],
    "license": "MIT",
    "homepage": "https://github.com/RiKaZarai/sylius-product-duplication-plugin",
    "authors": [
        {
            "name": "RiKaZarai",
            "homepage": "https://github.com/RiKaZarai"
        }
    ],
    "require": {
        "php": "^8.3",
        "sylius/sylius": "^2.0",
        "symfony/framework-bundle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/test-pack": "^1.0",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/browser-kit": "^7.3",
        "symfony/phpunit-bridge": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "RiKaZarai\\SyliusProductDuplicationPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiKaZarai\\SyliusProductDuplicationPlugin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "symfony": {
            "require": "^7.3"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    }
}
